  <view name="display" displayname="{t}Display{/t}">
  </view>
  <field name="Field" displayname="{t}Field{/t}" simple_type="text">
  </field>
  <field name="Type" displayname="{t}Type{/t}" simple_type="text">
  </field>
  <field name="Null" displayname="{t}Null{/t}" simple_type="text">
  </field>
  <field name="Key" displayname="{t}Key{/t}" simple_type="text">
  </field>
  <field name="Default" displayname="{t}Default{/t}" simple_type="text">
  </field>
  <field name="Extra">
  </field>
  <field name="Privileges">
  </field>
</table>