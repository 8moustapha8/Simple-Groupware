<table modulename="{t}Hosts{/t}" default_view="display" no_search_index="true">
  <view name="display" />
  <field name="serverid" simple_type="int">
    <KEY/>
  </field>
  <field name="hostip" simple_type="text" db_size="15">
    <KEY/>
  </field>
  <field name="created" simple_type="datetime" />
  <field name="lastmodified" simple_type="datetime" />
  <field name="id" simple_type="id" hidden="true"/>
</table>