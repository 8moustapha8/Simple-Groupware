<assets>
  <asset>
	<bookmarkname>1) {t}News{/t}</bookmarkname>
	<url>index.php?folder=^projects_@projectname@/!news&amp;view=display</url>
  </asset>
  <asset>
	<bookmarkname>2) {t}E-mail{/t}</bookmarkname>
	<url>index.php?folder=^projects_@projectname@/!emails&amp;view=display</url>
  </asset>
  <asset>
	<bookmarkname>3) {t}Calendar{/t}</bookmarkname>
	<url>index.php?folder=^projects_@projectname@/!calendar&amp;view=display&amp;markdate=week&amp;today=last monday</url>
  </asset>
  <asset>
	<bookmarkname>4) {t}Project tasks{/t}</bookmarkname>
	<url>index.php?folder=^projects_@projectname@/!tasks&amp;view=display&amp;find[]=assets|simple_tasks||project=@projectname@</url>
  </asset>
  <asset>
	<bookmarkname>5) {t}Project files{/t}</bookmarkname>
	<url>index.php?folder=^projects_@projectname@/!files&amp;view=display&amp;orderby=lastmodified&amp;order=desc<!--
		-->&amp;find[]=assets|simple_files||project=@projectname@</url>
  </asset>
  <asset>
	<bookmarkname>6) {t}Bookmarks{/t}</bookmarkname>
	<url>index.php?folder=^projects_@projectname@/!bookmarks&amp;view=display</url>
  </asset>
